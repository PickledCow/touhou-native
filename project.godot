[application]

config/name="Touhou"
run/main_scene="res://examples/mainmenu.tscn"
boot_splash/fullsize=false

[autoload]

Bullets="*res://addons/native_bullets/bullets.gdns"
Constants="*res://core/Constants.tscn"
DefSys="*res://default/DefSys.tscn"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/cloud/plugin.cfg", "res://addons/native_bullets/plugin.cfg", "res://addons/zylann.hterrain/plugin.cfg" )

